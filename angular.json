{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "jar-ngrx": {
      "projectType": "library",
      "root": "projects/jar-ngrx",
      "sourceRoot": "projects/jar-ngrx/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/jar-ngrx/tsconfig.lib.json",
            "project": "projects/jar-ngrx/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jar-ngrx/src/test.ts",
            "tsConfig": "projects/jar-ngrx/tsconfig.spec.json",
            "karmaConfig": "projects/jar-ngrx/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/jar-ngrx/tsconfig.lib.json",
              "projects/jar-ngrx/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "jar-playground": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/jar-playground",
      "sourceRoot": "projects/jar-playground/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/jar-playground",
            "index": "projects/jar-playground/src/index.html",
            "main": "projects/jar-playground/src/main.ts",
            "polyfills": "projects/jar-playground/src/polyfills.ts",
            "tsConfig": "projects/jar-playground/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/jar-playground/src/favicon.ico",
              "projects/jar-playground/src/assets"
            ],
            "styles": [
              "projects/jar-playground/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/jar-playground/src/environments/environment.ts",
                  "with": "projects/jar-playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "jar-playground:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "jar-playground:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jar-playground:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jar-playground/src/test.ts",
            "polyfills": "projects/jar-playground/src/polyfills.ts",
            "tsConfig": "projects/jar-playground/tsconfig.spec.json",
            "karmaConfig": "projects/jar-playground/karma.conf.js",
            "assets": [
              "projects/jar-playground/src/favicon.ico",
              "projects/jar-playground/src/assets"
            ],
            "styles": [
              "projects/jar-playground/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/jar-playground/tsconfig.app.json",
              "projects/jar-playground/tsconfig.spec.json",
              "projects/jar-playground/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/jar-playground/e2e/protractor.conf.js",
            "devServerTarget": "jar-playground:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jar-playground:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "jar-ngrx"
}